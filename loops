#!/bin/bash

for i in 1 2 3 
do
case $i in
    1) echo "First option";;
    2) echo "Second option";;
    3) echo "Third option";;
esac
done

# unset IFS
build_args () {
    args=() 
    for var in $(compgen -e | grep RDSL); do
        args+="$var=${!var}"
    done

    echo "docker build $args ."
}


# var=$(build_args)
